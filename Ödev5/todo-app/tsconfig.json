{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true
    },
    "include": ["src"]
  }